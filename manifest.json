{
  "manifest_version": 3,
  "name": "Release/Simulcast Calendar Helper for Crunchyroll",
  "description": "Starter template for a basic content scripts extension.",
  "version": "1.0.0",
  "icons": {
    "128": "images/icon.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "images/checkmark.svg",
        "font/*"
      ],
      "extension_ids": [
        "obpphalkbgbbdennbifedaleahjcfmfh"
      ],
      "matches": [
        "https://www.crunchyroll.com/*"
      ],
      "use_dynamic_url": true
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.crunchyroll.com/simulcastcalendar*"
      ],
      "css": [
        "css/fontello.css",
        "css/index.css"
      ],
      "js": [
        "js/vendors/caldom.min.js",
        "js/lib/constants.js",
        "js/lib/utils.js",
        "js/lib/event_handler.js",
        "js/lib/ui_modifier.js",
        "js/classes/content.js",
        "js/classes/day.js",
        "js/classes/week.js",
        "js/classes/filter.js",
        "js/contentScript.js"
      ]
    }
  ]
}